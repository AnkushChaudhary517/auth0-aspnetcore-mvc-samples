{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Auth0": {
    "Domain": "{DOMAIN}",
    "ClientId": "{CLIENT_ID}",
    "ClientSecret": "{CLIENT_SECRET}"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:3000"
      },
      "Https": {
        "Url": "https://*:3001"
      }
    }
  }
}